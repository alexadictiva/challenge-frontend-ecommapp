---
import Cuerpo from "../components/Cuerpo.astro";
import Card from "../components/Card.astro";
---

<div class="bg-blur-menu"></div>
<section id="wrapper">
  <!-- Sidebar -->
  <article class="menu-big-container">
    <div class="top-menu-content">
      <aside class="menu-hd-container">
        <button class="btn-float rotate-button">
          <img
            src="./../src/assets/img/menu-btn-deskt-ico.svg"
            alt="Arrow icon"
          />
        </button>
        <div class="btn-menu-container">
          <button class="btn-menu">
            <span class="span1"></span>
            <span class="span2"></span>
            <span class="span3"></span>
          </button>
        </div>
        <div class="logo-menu-container">
          <img src="./../src/assets/img/logo_full.svg" alt="Logo Ecomm App" />
          <img
            src="./../src/assets/img/logo_full-compress.svg"
            alt="Logo Ecomm App compress"
          />
        </div>
        <div class="btn-demo-container">
          <button class="btn-demo" id="Dropdown-user">
            <img
              src="./../src/assets/img/camera.png"
              alt="..."
              class="rounded-circle img-fluid profile-img p-0 p-md-2"
            />
          </button>
        </div>
        <div
          class="card-notification-user-mobile"
          aria-labelledby="Dropdown-user"
        >
          <div class="card-body text-center user-card p-3">
            <h5 class="card-title"></h5>
            <p class="mb-4">Administrador</p>
            <a href="Usuario/logout" class="btn-close-session">Cerrar sesión</a>
            <a
              href="javascript:void(0)"
              class="btn btn-primary btn-lg mt-2"
              style="display: none"
              onclick="modalMulticuenta()">Multicuenta</a
            >
          </div>
        </div>
      </aside>
      <aside class="menu-notifications-container-mobile">
        <ul class="notifications-content">
          <li class="icon-effects icon-sent">
            <img src="./../src/assets/img/sent-icon.svg" alt="Sending Icon" />
            <span
              class="badge sent-badge envios_pendientes_count"
              data-count="0">0</span
            >
          </li>
          <li class="icon-effects icon-question">
            <img src="./../src/assets/img/quest-icon.svg" alt="Question Icon" />
            <span
              class="badge question-badge notificaciones-preguntas-count"
              data-count="0">0</span
            >
          </li>
        </ul>
        <ul class="notifications-content notifications-content-user">
          <li class="px-2 icon icon-user">
            <div id="Dropdown-user" class="btn-demo-desktop">
              <span
                class="d-inline-block font-weight-bold username mr-0"
                style="font-size: 1rem;"
              >
              </span>
              <img
                src="./../src/assets/img/camera.png"
                alt="..."
                class="rounded-circle img-fluid profile-img p-0 p-md-2"
              />
            </div>
            <div
              class="card-notification-desktop"
              aria-labelledby="Dropdown-user"
            >
              <div class="card-body text-center user-card p-3">
                <h5 class="card-title"></h5>
                <p class="mb-4">Administrador</p>
                <a href="Usuario/logout" class="btn-close-session"
                  >Cerrar sesión</a
                >
                <a
                  href="javascript:void(0)"
                  class="btn btn-primary btn-lg mt-2"
                  style="display: none"
                  onclick="modalMulticuenta()">Multicuenta</a
                >
              </div>
            </div>
          </li>
        </ul>
      </aside>
    </div>
    <nav class="sidebar-wrapper" id="sidebar-wrapper">
      <ul class="menu" id="menuacordeon">
        <li
          class="item-menu meli-container dropdown navbar-button-mercadolibre collapsed"
          data-toggle="collapse"
          data-target="#data_ml"
          aria-expanded=" false"
          aria-controls="data_ml"
        >
          <div class="link-menu link-menu-special">
            <div class="img-container">
              <img
                src="./../src/assets/img/meli-white-icon.svg"
                alt="MercadoLibre Icon"
              />
            </div>
            <div class="noti-div d-none"></div>
            <div class="tooltip-sidebar">
              <a class="toltip">Mercado Libre</a>
            </div>

            <span
              >Mercado Libre
              <strong
                data-count="0"
                class="ml-1 d-none notificaciones-meli-count">0</strong
              >
            </span>
          </div>
          <ul
            id="data_ml"
            class="submenu collapse"
            aria-labelledby="menu_ml"
            data-parent="#menuacordeon"
          >
            <form>
              <li
                class="submenu-item audit-tracker"
                data-module="Publicaciones_meli"
                data-method="index"
                onclick="window.location.href = 'Publicaciones_meli';"
              >
                <div class="img-container">
                  <img
                    src="./../src/assets/img/pub-icon.svg"
                    alt="Publications Icon"
                  />
                </div>
                <div class="tooltip-sidebar">
                  <a class="toltip">Publicaciones</a>
                </div>
                <span>Publicaciones</span>
              </li>
            </form>
          </ul>
        </li>

        <li
          class="item-menu Inventario-container dropdown navbar-button-Inventario"
        >
          <div class="link-menu">
            <div class="img-container">
              <img
                src="./../src/assets/img/invent-icon.svg"
                alt="Inventario Icon"
              />
            </div>
            <div class="tooltip-sidebar"><a class="toltip">Inventario</a></div>
            <span>Inventario</span>
          </div>
        </li>
      </ul>
    </nav>
  </article>
  <!-- /#sidebar-wrapper -->
  <!-- Page Content -->

  <div id="page-content-wrapper" class="dashboard mb-5">
    <nav
      class="d-none d-lg-block navbar navbar-expand-lg border-bottom nav-desktop"
      style="gap: 8px;"
    >
      <div class="row no-gutters">
        <div
          class="col-12 justify-content-end d-flex"
          id="navbarSupportedContent"
        >
        </div>
      </div>
    </nav>
    <section class="cuerpo-container">
      <Cuerpo />
      <div class="cards-container">
        <Card />
        <Card />
        <Card />
        <Card />
        <Card />
        <Card />
        <Card />
      </div>
    </section>
  </div>
</section>
<style>
  .bg-blur-menu {
    background: black;
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    z-index: 1001;
    top: 0;
    opacity: 0.5;
    display: none;
  }

  .active-blur {
    display: block;
  }

  @media screen and (min-width: 1024px) {
    .desktop-hidden {
      display: none !important;
    }
  }

  .mobile-none {
    display: none !important;
  }
  @media screen and (min-width: 1024px) {
    .mobile-none {
      display: block !important;
    }
  }

  .comprobantes_emitidos tbody tr td {
    font-size: 14px;
  }

  .table thead th {
    font-size: 14px;
    vertical-align: middle;
  }

  #page-content-wrapper {
    width: 100%;
  }

  .topcard {
    color: #fff;
  }

  /*Nuevo presupuesto provisional*/
  .title-general {
    display: inline-block;
  }

  #subir-excel-masivo,
  #btn-upld-file,
  #btn-dwld-file,
  .btn-descargar,
  #subir-excel-masivo-2 {
    transition: all 0.3s linear;
  }
  #subir-excel-masivo:hover,
  #btn-upld-file:hover,
  #btn-dwld-file:hover,
  .btn-descargar:hover,
  #subir-excel-masivo-2:hover {
    background: #4871bf !important;
  }

  .tooltip-search-compet {
    display: none;
    padding: 5px 15px;
    text-align: center;
    position: absolute;
    z-index: 5;
    width: 285px;
    background: #304b7f;
    border-radius: 19.8305px;
    top: 155px;
    left: 45px;
  }
  @media screen and (min-width: 1024px) {
    .tooltip-search-compet {
      width: 275px;
      left: auto;
      right: 380px;
      top: 170px;
    }
  }
  .tooltip-search-compet:before {
    content: "";
    position: absolute;
    width: 20px;
    height: 20px;
    transform: rotate(45deg);
    background-color: #304b7f;
    top: -10px;
    left: 47%;
  }
  @media screen and (min-width: 1024px) {
    .tooltip-search-compet:before {
      top: -10px;
      left: 50px;
    }
  }
  .tooltip-search-compet p {
    text-align: start;
    font-weight: 500;
    font-size: 19.1899px;
    line-height: 26px;
    color: #ffffff;
    margin-bottom: 17px;
  }
  @media screen and (min-width: 1024px) {
    .tooltip-search-compet p {
      font-size: 18px;
      line-height: 23px;
    }
  }
  .tooltip-search-compet img {
    width: 245px;
  }

  #btn-flotante {
    position: absolute;
    right: 30px;
    top: 82px;
    width: 300px;
    height: 10px;
    background-color: transparent;
    border: 0;
    outline: none;
  }
  #btn-flotante:focus-visible,
  #btn-flotante:focus {
    border: 0;
    outline: none;
  }
  @media screen and (min-width: 1024px) {
    #btn-flotante {
      width: 600px;
      right: 300px;
    }
  }
  @media screen and (min-width: 1024px) {
    #btn-flotante {
      right: 600px;
    }
  }

  .no-cuit-label {
    font-weight: 400;
    font-size: 12px;
    color: #727587;
    margin-left: 5px;
  }

  .special-navbar {
    height: 0;
  }
  @media screen and (min-width: 1024px) {
    .special-navbar {
      height: 55px;
      gap: 8px;
    }
  }

  /* =====================================
AGREGAR DESCUENTO EN EMITIR COMPROBANTE
===================================== */
  .resumen-comp-container {
    width: 100%;
  }
  @media screen and (min-width: 1024px) {
    .resumen-comp-container {
      width: 96%;
      margin: auto;
      margin-right: auto !important;
      margin-left: auto !important;
    }
  }
  @media screen and (min-width: 1660px) {
    .resumen-comp-container {
      width: 59%;
    }
  }
  .resumen-comp-container .form-group {
    margin-bottom: 5px;
  }

  .column-padding {
    padding-left: 15px;
    padding-right: 0px;
  }

  .tituloInput {
    font-weight: 600;
    font-size: 12.5px;
    color: #1d263f;
    margin-bottom: 0;
  }
  @media screen and (min-width: 1024px) {
    .tituloInput {
      font-size: 15px;
      margin-bottom: 1rem;
    }
  }

  .input-add-descuento {
    background: #ffffff;
    border: 1px solid #ced4d9;
    outline: none;
    width: 50px;
    height: 35px;
    margin-top: -5px;
  }
  @media screen and (min-width: 1024px) {
    .input-add-descuento {
      width: 72px;
    }
  }
  .input-add-descuento:focus,
  .input-add-descuento:focus-visible {
    outline: none;
  }
  .input-add-descuento:hover {
    border: 1px solid #6097ff;
    box-shadow: 0px 0px 4px #b0cbff;
  }

  .form-control-emitir-comprobante {
    background: #fff;
    border: 0;
    font-weight: 400;
    font-size: 16px;
    color: #43475f;
    width: 100%;
    cursor: default;
  }
  .form-control-emitir-comprobante:focus,
  .form-control-emitir-comprobante:focus-visible {
    outline: none;
  }

  .form-control-emitir-comprobante-total {
    font-weight: 800;
    font-size: 16px;
    letter-spacing: 0.02em;
    color: #43475f;
    border: 0;
    width: 100%;
    cursor: default;
  }
  .form-control-emitir-comprobante-total:focus,
  .form-control-emitir-comprobante-total:focus-visible {
    outline: none;
  }

  .preguntic-descuento:hover ~ .tooltip-info {
    display: block;
  }

  .tooltip-info {
    background-color: #121c30;
    border-radius: 10px;
    padding: 5px;
    position: absolute;
    font-weight: 500;
    width: 250px;
    top: -112px;
    right: -88px;
    color: #fff;
    font-size: 13px;
    line-height: 16px;
    z-index: 1;
    display: none;
  }
  @media screen and (min-width: 1024px) {
    .tooltip-info {
      width: 300px;
      top: -96px;
      right: -15px;
    }
  }
  @media screen and (min-width: 1240px) {
    .tooltip-info {
      right: -13px;
    }
  }
  @media screen and (min-width: 1366px) {
    .tooltip-info {
      right: 9px;
    }
  }
  .tooltip-info:before {
    content: "";
    position: absolute;
    bottom: -5px;
    right: 92px;
    width: 10px;
    height: 10px;
    transform: rotate(45deg);
    background-color: #121c30;
  }
  @media screen and (min-width: 1024px) {
    .tooltip-info:before {
      right: 18px;
    }
  }

  .container-importe-total {
    border-top: 0.5px solid #c2c2c2;
    padding-top: 10px;
  }

  .input-add-descuento::-webkit-outer-spin-button,
  .form-control-emitir-comprobante::-webkit-outer-spin-button,
  .form-control-emitir-comprobante-total::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }

  .input-add-descuento,
  .form-control-emitir-comprobante,
  .form-control-emitir-comprobante-total {
    -moz-appearance: textfield;
  }

  .clear-filter-btn.hovered:not(.clear-filter-btn-old) {
    display: flex;
    height: 34px;
  }

  #limpiar_filtros.hovered,
  .clear-filter-btn-old.hovered {
    color: #fff !important;
    background: linear-gradient(
      to bottom,
      #c34c4c 0%,
      #da6666 44%,
      #f38383 100%
    ) !important;
  }

  .btn-delete-chip {
    font-weight: 500;
    font-size: 14px;
    color: #304b7f;
    background: #fff;
    border: 0;
    height: 21px;
    transition: all 0.2s linear;
    box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.2);
    border-radius: 4px;
  }
  .btn-delete-chip:focus,
  .btn-delete-chip:focus-visible {
    outline: none;
    border: 0;
  }
  .btn-delete-chip:hover {
    text-decoration: underline;
  }

  .full-tag {
    font-style: italic;
    font-weight: 500;
    font-size: 12px;
    color: #4aa359;
    background: #ffffff;
    border: 0.5px solid #4aa359;
    border-radius: 2px;
    padding: 0 6px;
    margin-bottom: 5px;
    display: inline-block;
    cursor: default;
  }
  .full-tag img {
    width: 10.5px;
  }

  .flex-tag-active {
    font-style: italic;
    font-weight: 500;
    font-size: 12px;
    color: #ffffff;
    background: #4aa359;
    border: 0.5px solid #4aa359;
    border-radius: 2px;
    padding: 0 6px;
    margin-bottom: 5px;
    display: inline-block;
    margin-left: 5px;
    cursor: default;
  }

  .flex-tag-inactive {
    font-style: italic;
    font-weight: 500;
    font-size: 12px;
    color: #ffffff;
    background: #727587;
    border: 0.5px solid #727587;
    border-radius: 2px;
    padding: 0 6px;
    margin-bottom: 5px;
    display: inline-block;
    margin-left: 5px;
    position: relative;
    cursor: default;
  }
  .flex-tag-inactive:hover > .tooltip-flex {
    display: block;
  }
  .flex-tag-inactive img {
    width: 13px;
  }
  .flex-tag-inactive .tooltip-flex {
    display: none;
    position: absolute;
    border-radius: 4px;
    background-color: #141d2f;
    padding: 4px 8px;
    width: 215px;
    left: -60px;
    top: 30px;
    font-style: normal;
  }
  @media screen and (min-width: 1024px) {
    .flex-tag-inactive .tooltip-flex {
      left: -70px;
    }
  }
  .flex-tag-inactive .tooltip-flex:before {
    content: "";
    background-color: #141d2f;
    width: 12px;
    height: 12px;
    transform: rotate(45deg) translateX(-50%);
    position: absolute;
    top: -2px;
    left: 45%;
    z-index: 0;
  }

  .hidden1 {
    display: none;
  }

  .bloqueado {
    background: #e5e6ee !important;
  }

  /* comprobanes emitidos responsive */
  .table-comp-emit {
    white-space: nowrap;
  }
  @media screen and (min-width: 1024px) {
    .table-comp-emit {
      white-space: normal;
    }
  }
  .table-comp-emit i.fas:before {
    font-size: 11.5px;
  }

  @media screen and (min-width: 1024px) {
    .tbl-name-com-emit {
      width: 210px !important;
    }
  }
  @media screen and (min-width: 1660px) {
    .tbl-name-com-emit {
      width: auto !important;
    }
  }

  .formConcepto {
    font-size: 14px;
  }

  .ventas-old-container .alert-filters-container {
    display: flex;
    gap: 10px;
    flex-direction: column;
    align-items: start;
    margin-top: 10px;
  }
  @media screen and (min-width: 768px) {
    .ventas-old-container .alert-filters-container {
      flex-direction: row;
      flex-wrap: wrap;
    }
  }
  .ventas-old-container .alert-filters-container .alert {
    width: max-content;
    margin: 0;
  }
  .ventas-old-container .alert-filters-container .alert.alert-filter-pub {
    background: #d7e5ff;
    border-radius: 4px;
    padding: 2px 8px;
    display: flex;
    align-items: center;
    height: 36px;
    gap: 5px;
    transition: all 0.2s linear;
  }
  .ventas-old-container .alert-filters-container .alert.alert-filter-pub:hover {
    background: #b0cbff;
  }
  @media screen and (min-width: 1024px) {
    .ventas-old-container .alert-filters-container .alert.alert-filter-pub {
      padding: 2px 4px;
      height: 21px;
    }
  }
  .ventas-old-container .alert-filters-container .alert.alert-filter-pub p {
    margin: 0;
    display: inline-block;
    font-weight: 300;
    font-size: 14px;
    line-height: 21px;
    color: #304b7f;
  }
  .ventas-old-container
    .alert-filters-container
    .alert.alert-filter-pub
    button {
    padding: 0;
    margin: 0;
    font-weight: 300;
    font-size: 22px;
    color: #304b7f;
  }

  .alert-filters-container1 {
    display: flex;
    gap: 10px;
    flex-direction: row;
    align-items: start;
    margin-top: 10px;
  }
  .alert-filters-container1 .alert-filter-pub {
    background: #d7e5ff;
    border-radius: 4px;
    padding: 2px 8px;
    display: flex;
    align-items: center;
    height: 30px;
    gap: 5px;
    transition: all 0.2s linear;
  }
  .alert-filters-container1 .alert-filter-pub p {
    margin: 0;
    display: inline-block;
    font-weight: 300;
    font-size: 14px;
    line-height: 21px;
    color: #304b7f;
  }
  .alert-filters-container1 .alert-filter-pub button {
    margin: 0;
  }
  .alert-filters-container1 .alert-filter-pub button span {
    padding: 0;
    margin: 0;
    font-weight: 300;
    font-size: 22px;
    color: #304b7f;
  }

  /*tags*/
  p.tags-container1 .tag-general {
    font-style: italic;
    font-weight: 500;
    font-size: 12px;
    color: #ffffff;
    border-radius: 2px;
    padding: 1px 4px;
    margin-bottom: 4px;
    display: inline-block;
  }
  p.tags-container1 .tag-general.tag-full {
    color: #4aa359;
    border: 0.5px solid #4aa359;
  }
  p.tags-container1 .tag-general.tag-flex {
    background: #4aa359;
    border: 0.5px solid #4aa359;
  }
  p.tags-container1 .tag-general.tag-cross {
    background: #ffb660;
  }
  p.tags-container1 .tag-general.tag-drop {
    background: #58c6ab;
  }
  p.tags-container1 .tag-general.tag-dropx {
    background: #ff7b7b;
  }
  p.tags-container1 .tag-general.tag-acord {
    background: #6097ff;
  }
  p.tags-container1 .tag-general img {
    width: 10px;
  }

  /*input search general*/
  .input-search-general-container {
    border: #6097ff solid 1px;
    border-radius: 5px;
  }
  @media screen and (min-width: 1024px) {
    .input-search-general-container {
      width: 630px;
    }
  }
  .input-search-general-container .input-search-general {
    border: 0 !important;
    filter: none !important;
    border-radius: 5px;
    height: 36px;
  }
  .input-search-general-container .input-search-general.form-control:focus {
    box-shadow: none;
  }
  .input-search-general-container #inputDelete {
    border: 0;
    outline: none;
    background: #fff;
    padding-right: 10px;
  }
  .input-search-general-container #inputDelete svg {
    opacity: 0;
  }
  .input-search-general-container .input-group-append-general {
    background: transparent;
    position: relative;
  }
  .input-search-general-container .input-group-append-general .btn-buscador {
    background: #6097ff;
    color: #fff;
    border-radius: 0 4px 4px 0;
    border: 0;
    cursor: pointer;
  }
  .input-search-general-container
    .input-group-append-general
    .btn-buscador:hover {
    background: #4871bf;
  }

  .tachado {
    text-decoration: line-through;
  }

  /*NC*/
  .custom-controls-unhover-variante {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    margin-top: 17px;
  }
  .custom-controls-unhover-variante .nc-container p {
    cursor: pointer;
    display: flex;
    align-items: center;
    gap: 5px;
    padding: 10px 9px;
    width: max-content;
    border-radius: 3.456px;
    background: #fff;
    box-shadow: 1px 1px 8px 0px rgba(0, 0, 0, 0.1490196078);
    color: #7f3030;
    font-size: 13px;
    font-weight: 500;
    letter-spacing: -0.207px;
    margin: 0;
  }
  .custom-controls-unhover-variante .nc-container p:hover {
