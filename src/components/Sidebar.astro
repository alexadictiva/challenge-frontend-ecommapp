<script type="module">
  let btnMenu = document.querySelector(".btn-menu");
  btnMenu.addEventListener("click", function (event) {
    document.querySelector(".sidebar-wrapper").classList.toggle("show-menu");
    document.querySelector(".span1").classList.toggle("rotate-left");
    document.querySelector(".span2").classList.toggle("hidden");
    document.querySelector(".span3").classList.toggle("rotate-right");
    document.querySelector(".bg-blur-menu").classList.toggle("active-blur");
  });
  let btnDemo = document.querySelector(".btn-demo");
  btnDemo.addEventListener("click", function (event) {
    var cardNotification = document.querySelector(
      ".card-notification-user-mobile"
    );
    if (cardNotification.classList.contains("mostrar")) {
      cardNotification.classList.remove("mostrar");
    } else {
      cardNotification.classList.add("mostrar");
    }
  });

  let btnDemoDesktop = document.querySelector(".btn-demo-desktop");
  btnDemoDesktop.addEventListener("click", function (event) {
    document
      .querySelector(".card-notification-desktop")
      .classList.add("mostrar");
    document.querySelector(".icon-user").classList.add("active");
  });

  let btnFloat = document.querySelector(".btn-float");
  btnFloat.addEventListener("click", function (event) {
    var menuBigContainer = document.querySelector(".menu-big-container");
    var btnFloat = document.querySelector(".btn-float");
    var miniDropSidebar = document.querySelector(".mini-drop-sidebar");

    menuBigContainer.classList.toggle("desactive");
    btnFloat.classList.toggle("rotate-down");
    btnFloat.classList.toggle("rotate-button");
  });
</script>
